{
    "name": "teleboy/symfony-messenger-pubsub-bridge",
    "description": "Bridging Symfony Messenger with Google Cloud Pub/Sub",
    "type": "symfony-bridge",
    "license": "MIT",
    "authors": [
        {
            "name": "Cedric Ziel",
            "email": "mail@cedric-ziel.com"
        },
        {
            "name": "Teleboy",
            "email": "dev@teleboy.ch"
        }
    ],
    "autoload": {
        "psr-4": {
            "CedricZiel\\Symfony\\Messenger\\Bridge\\GcpPubSub\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "require": {
        "php": ">=7.4",
        "google/cloud-pubsub": "^1.29",
        "psr/log": "^1.1",
        "symfony/event-dispatcher-contracts": "^2.4",
        "symfony/http-kernel": "^4.4|^5.3",
        "symfony/messenger": "^4.4|^5.3"
    },
    "require-dev": {
        "jangregor/phpstan-prophecy": "^0.8.1",
        "phpspec/prophecy-phpunit": "^2.0",
        "phpstan/phpstan": "^0.12.91",
        "phpstan/phpstan-phpunit": "^0.12.20",
        "phpunit/phpunit": "^9.5",
        "symfony/event-dispatcher": "^4.4|^5.0",
        "symfony/phpunit-bridge": "^4.4 || ^5.0",
        "symfony/process": "^4.4|^5.0",
        "symfony/property-access": "^4.4|^5.0",
        "symfony/serializer": "^4.4|^5.0"
    },
    "suggest": {
        "ext-grpc": "Improved performance for gRPC connections",
        "ext-protobuf": "Improved performance for gRPC connections"
    },
    "scripts": {
        "test": "@php vendor/phpunit/phpunit/phpunit",
        "phpstan": "@php -d memory_limit=-1 vendor/phpstan/phpstan/phpstan analyse"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
